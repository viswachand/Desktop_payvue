{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "ignoreDeprecations": "6.0",

    "paths": {
      "@/*": ["src/*"]
    },
    "jsx": "react-jsx",
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": true,
    "allowImportingTsExtensions": true,
    "strict": true,
    "skipLibCheck": true
  },
    "include": ["src", "src/vite-env.d.ts"]
}
